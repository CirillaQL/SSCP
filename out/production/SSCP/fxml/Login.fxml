<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: rgba(205,205,205,0.5);" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.LoginController">
