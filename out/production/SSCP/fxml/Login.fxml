      <AnchorPane prefHeight="115.0" prefWidth="600.0" style="-fx-background-color: rgba(34,166,242,0.7);">
         <children>
            <ImageView fx:id="image" fitHeight="115.0" fitWidth="100.0" layoutX="82.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../resource/coconut-drink-icon.png" />
               </image>
            </ImageView>
         </children></AnchorPane>
      <Button fx:id="confirm" layoutX="87.0" layoutY="298.0" mnemonicParsing="false" onAction="#print" prefHeight="35.0" prefWidth="100.0" stylesheets="@../resource/button.css" text="确定" wrapText="true">
